<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jk.mapper.hgcMapper.IOrderMapper">
<select id="queryorderlist" resultType="com.jk.model.hgcmodel.Order">
    select t.*, t.rowid from t_order t

</select>
    <select id="queryorderbyid" resultType="com.jk.model.hgcmodel.Order">
        select t.*  from t_order t where t.id=#{id}

    </select>
</mapper>